{
  "name": "plaything",
  "version": "0.0.1",
  "description": "Playing around",
  "main": "src/index.ts",
  "author": "Zac Seth",
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "@types/enzyme": "^3.1.4",
    "@types/express": "^4.0.39",
    "@types/jest": "^21.1.6",
    "@types/react-router-dom": "^4.2.2",
    "awesome-typescript-loader": "^3.4.0",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "es-abstract": "^1.9.0",
    "jest": "^21.2.1",
    "react-test-renderer": "^16.1.1",
    "source-map-loader": "^0.2.3",
    "ts-jest": "^21.2.3",
    "typescript": "^2.6.1",
    "webpack": "^3.8.1"
  },
  "dependencies": {
    "@types/react": "^16.0.25",
    "@types/react-dom": "^16.0.3",
    "express": "^4.16.2",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-router-dom": "^4.2.2"
  },
  "scripts": {
    "test": "jest",
    "build": "webpack"
  },
  "homepage": "https://zac-seth.github.io/plaything/",
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/config/jest/mocks/fileMock.ts",
      "\\.(css|less)$": "<rootDir>/config/jest/mocks/styleMock.ts",
      
      "^elements/(.*)$": "<rootDir>/src/elements/$1",
      "^public/(.*)$": "<rootDir>/src/public/$1"
    },
    "setupFiles": [
      "<rootDir>/config/jest/jest.polyfill.ts",
      "<rootDir>/config/jest/jest.setup.ts"
    ],
    "testRegex": "(/_specs/.*|\\.(spec))\\.(ts|tsx|js)$",
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    }
  }
}
