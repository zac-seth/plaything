{
  "defaultSeverity": "error",
  "extends": [
      "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "arrow-parens": false,
    "indent": {
      "options": [
        "spaces",
        2
      ]
    },
    "prefer-template": true,
    "trailing-comma": [
      true,
      {
        "multiline": {
          "objects": "always",
          "arrays": "always",
          "functions": "never",
          "typeLiterals": "ignore"
        },
        "esSpecCompliant": true
      }
    ]
  },
  "rulesDirectory": []
}